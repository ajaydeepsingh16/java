import java.util.*;
class  Main{
    public static void main(String [] args)
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int ar[]=new int[n];
        for(int i=0;i<n;i++)
        {
            ar[i]=s.nextInt();
        }
        Arrays.sort(ar);
        System.out.println("Second largest Element of array is: "+ar[n-2]);
        System.out.println("Second smalles Element of array is: "+ar[1]);
    }
}
